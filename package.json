    "@vitejs/plugin-react-refresh": "^1.3.5",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.5.4",
    "vite": "2",
    "vite-plugin-pwa": "^0.11.13"
