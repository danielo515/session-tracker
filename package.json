  "scripts": {
    "start-webpack": "node scripts/start.js",
    "build-webpack": "node scripts/build.js",
    "build-types": "tsc  --noEmit false --emitDeclarationOnly true",
    "test": "node scripts/test.js --env=jsdom",
    "s3-deploy": "a(){ aws s3 sync build s3://$1/; };a",
    "deploy": "yarn build && yarn s3-deploy",
    "//": "adds a component in form of feature/component",
    "add-component": "npx rekit add component -t functional",
    "///": "adds a page to a feature. Make sure to call it with url feature/name ",
    "add-page": "npx rekit add component -t functional -c -u",
    "////": "adds a new async action, feature/action",
    "add-action": "npx rekit add action -a ",
    "lint": "eslint --ext ts,tsx src",
    "dev": "vite",
    "start": "vite",
    "preview": "vite preview",
    "build": "vite build"
  },
  "rekit": {
    "devPort": 6075,
    "studioPort": 6076,
    "plugins": [],
    "css": "less"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "<rootDir>/**/*.test.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jest/recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "description": "time-traker created by Rekit.",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
